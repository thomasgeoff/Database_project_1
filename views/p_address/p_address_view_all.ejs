<!DOCTYPE html>
</html>
<head>
    <title>Geoff's Project 1</title>

</head>
<body>
        <div >
            <table border="1" cellpadding="7" cellspacing="7">
                <tr>
                    <th>Property name</th>
                    <th>City</th>
                    <th>Zip</th>
                    <th width="120px">Action</th>
                </tr>
                <% for(var i=0; p_addresses.length > i; i++) { %>
                <tr>
                    <td><a href="/p_address/?p_address_id=<%= p_addresses[i].p_address_id %>">
                            <%= p_addresses[i].property_name %>
                        </a>
                    </td>
                    <td><a href="/p_address/?p_address_id=<%= p_addresses[i].p_address_id %>">
                            <%= p_addresses[i].city %>
                        </a>
                    </td>
                    <td><a href="/p_address/?p_address_id=<%= p_addresses[i].p_address_id %>">
                            <%= p_addresses[i].zip %>
                        </a>
                    </td>
                    <td>
                        <a href='/p_address/edit/?p_address_id=<%= p_addresses[i].p_address_id %>'>
                            Edit
                        </a>
                    </td>
                </tr>
                <% }%>

            </table>
         </div>
        </div>
</body>
</html>

