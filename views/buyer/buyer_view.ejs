<!DOCTYPE html>
</html>
<head>
    <title>Buyers View</title>

</head>
<body>
<div >
    <table border="1" cellpadding="7" cellspacing="7">
        <tr>
            <th>User ID</th>
            <th>Property Type</th>
            <th>Budget</th>
            <th width="120px">Action</th>
        </tr>
        <% for(var i=0; buyers.length > i; i++) { %>
        <tr>
            <td><a href="/p_address/?p_address_id=<%= buyers[i].user_id %>">
                    <%= buyers[i].user_id %>
                </a>
            </td>
            <td><a href="/p_address/?p_address_id=<%= buyers[i].user_id %>">
                    <%= buyers[i].prop_type %>
                </a>
            </td>
            <td><a href="/p_address/?p_address_id=<%= buyers[i].user_id %>">
                    <%= buyers[i].budget %>
                </a>
            </td>
            <td>
                <a href='/p_address/edit/?p_address_id=<%= buyers[i].user_id %>'>
                    Edit
                </a>
            </td>
        </tr>
        <% }%>

    </table>
</div>
</div>
</body>
</html>

